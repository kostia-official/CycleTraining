<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.kozzztya.cycletraining"
          android:versionCode="1"
          android:versionName="1.0">

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="19"/>
    <uses-permission android:name="android.permission.VIBRATE"/>
    <application
        android:label="@string/app_name"
        android:icon="@drawable/ic_launcher"
        android:allowBackup="true"
        android:theme="@style/Theme.MyAppCompat"
        >
        <activity android:name=".DrawerActivity"/>
        <activity android:name=".trainingcreate.TrainingCreateActivity"/>
        <activity android:name=".trainingcreate.TrainingPlanActivity"/>
        <activity
            android:name=".trainingjournal.TrainingJournalActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".trainingjournal.TrainingDayActivity"/>
        <activity android:name=".trainingprocess.TrainingProcessActivity"/>
        <activity android:name=".MyPreferenceActivity"/>
        <activity android:name=".trainingcreate.ProgramsActivity"/>
        <activity android:name=".trainingcreate.ExercisesSearchActivity"/>
        <activity android:name=".statistic.StatisticShowActivity"/>
        <activity android:name=".statistic.StatisticCreateActivity"/>
        <activity android:name="org.achartengine.GraphicalActivity"/>
        <activity android:name=".trainingcreate.ExerciseCreateActivity"/>
    </application>
</manifest>
