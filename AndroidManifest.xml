<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.kozzztya.cycletraining"
          android:versionCode="1"
          android:versionName="1.0">

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="19"/>
    <application
        android:label="@string/app_name"
        android:icon="@drawable/ic_launcher"
        android:allowBackup="true"
        android:theme="@style/Theme.MyAppCompat"
        >
        <activity android:name=".DrawerActivity"/>
        <activity android:name=".trainingadd.TrainingAddActivity"/>
        <activity android:name=".trainingadd.TrainingPlanActivity"/>
        <activity
            android:name=".trainingjournal.TrainingJournalActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".trainingjournal.TrainingDayActivity"/>
        <activity android:name=".trainingprocess.TrainingProcessActivity"/>
        <activity android:name=".Preferences"/>
        <activity android:name=".trainingadd.ProgramsSearchActivity"/>
        <activity android:name=".trainingadd.ExercisesSearchActivity"/>
    </application>
</manifest>
