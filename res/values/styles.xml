<?xml version="1.0" encoding="utf-8"?>

<!-- Generated with http://android-holo-colors.com -->
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="Theme.MyAppCompat" parent="Theme.AppCompat.Light.DarkActionBar">

        <item name="android:editTextStyle">@style/EditTextMyAppCompat</item>
        <item name="android:buttonStyle">@style/ButtonMyAppCompat</item>
        <item name="android:imageButtonStyle">@style/ImageButtonMyAppCompat</item>
        <item name="android:spinnerStyle">@style/SpinnerMyAppCompat</item>
        <item name="android:spinnerItemStyle">@style/SpinnerItemMyAppCompat</item>
        <item name="android:spinnerDropDownItemStyle">@style/SpinnerDropDownItemMyAppCompat</item>
        <item name="android:listViewStyle">@style/ListViewMyAppCompat</item>

        <!-- ActionBar items -->
        <item name="actionBarItemBackground">@drawable/selectable_background_myappcompat</item>
        <item name="actionBarTabStyle">@style/ActionBarTabStyle.MyAppCompat</item>
        <item name="actionDropDownStyle">@style/DropDownNav.MyAppCompat</item>
        <item name="actionBarStyle">@style/ActionBar.Solid.MyAppCompat</item>
        <item name="actionModeBackground">@drawable/cab_background_top_myappcompat</item>
        <item name="actionModeSplitBackground">@drawable/cab_background_bottom_myappcompat</item>
        <item name="actionModeCloseButtonStyle">@style/ActionButton.CloseMode.MyAppCompat</item>
    </style>

    <style name="EditTextMyAppCompat" parent="android:Widget.EditText">
        <item name="android:background">@drawable/myappcompat_edit_text_holo_light</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="ButtonMyAppCompat" parent="android:Widget.Button">
        <item name="android:background">@drawable/myappcompat_btn_default_holo_light</item>
        <item name="android:minHeight">48dip</item>
        <item name="android:minWidth">64dip</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="ImageButtonMyAppCompat" parent="android:Widget.ImageButton">
        <item name="android:background">@drawable/myappcompat_btn_default_holo_light</item>
    </style>

    <style name="SpinnerMyAppCompat" parent="android:Widget.Spinner">
        <item name="android:background">@drawable/myappcompat_spinner_background_holo_light</item>
        <item name="android:dropDownSelector">@drawable/myappcompat_list_selector_holo_light</item>
    </style>

    <style name="SpinnerItemMyAppCompat"
           parent="android:TextAppearance.Widget.TextView.SpinnerItem">
        <item name="android:textColor">#000000</item>
        <item name="android:paddingLeft">@dimen/card_margin</item>
    </style>

    <style name="SpinnerDropDownItemMyAppCompat" parent="android:Widget.DropDownItem.Spinner">
        <item name="android:checkMark">@drawable/myappcompat_btn_radio_holo_light</item>
    </style>

    <style name="ActionBar.Solid.MyAppCompat"
           parent="@style/Widget.AppCompat.Light.ActionBar.Solid.Inverse">
        <item name="background">@drawable/ab_solid_myappcompat</item>
        <item name="backgroundStacked">@drawable/ab_stacked_solid_myappcompat</item>
        <item name="backgroundSplit">@drawable/ab_bottom_solid_myappcompat</item>
        <item name="progressBarStyle">@style/ProgressBar.MyAppCompat</item>
    </style>

    <style name="ActionBar.Transparent.MyAppCompat" parent="@style/Widget.AppCompat.ActionBar">
        <item name="background">@drawable/ab_transparent_myappcompat</item>
        <item name="progressBarStyle">@style/ProgressBar.MyAppCompat</item>
    </style>

    <style name="ActionBarTabStyle.MyAppCompat" parent="@style/Widget.AppCompat.ActionBar.TabView">
        <item name="android:background">@drawable/tab_indicator_ab_myappcompat</item>
    </style>

    <style name="DropDownNav.MyAppCompat"
           parent="@style/Widget.AppCompat.Spinner.DropDown.ActionBar">
        <item name="android:background">@drawable/spinner_background_ab_myappcompat</item>
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_myappcompat</item>
        <item name="android:dropDownSelector">@drawable/selectable_background_myappcompat</item>
    </style>

    <style name="ProgressBar.MyAppCompat" parent="@style/Widget.AppCompat.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/progress_horizontal_myappcompat</item>
    </style>

    <style name="ActionButton.CloseMode.MyAppCompat"
           parent="@style/Widget.AppCompat.ActionButton.CloseMode">
        <item name="android:background">@drawable/btn_cab_done_myappcompat</item>
    </style>

    <style name="TextAppearance.ShowcaseView.Detail" parent="android:style/TextAppearance">
        <item name="android:textColor">#555555</item>
    </style>

    <style name="ShowcaseView">
        <item name="sv_titleTextAppearance">@style/TextAppearance.ShowcaseView.Title</item>
        <item name="sv_detailTextAppearance">@style/TextAppearance.ShowcaseView.Detail</item>
        <item name="sv_backgroundColor">#20323232</item>
        <item name="sv_buttonText">@string/ok</item>
    </style>

    <style name="TextAppearance.ShowcaseView.Title" parent="android:style/TextAppearance.Large">
        <item name="android:textColor">#ff000000</item>
    </style>

    <style name="SectionHeader" parent="android:Widget.TextView">
        <item name="android:paddingLeft">12.5dp</item>
        <item name="android:layout_marginTop">@dimen/card_margin</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/dark_gray</item>
        <item name="android:textSize">14sp</item>
        <item name="textAllCaps">true</item>
    </style>

    <style name="HeaderDivider">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">2dp</item>
        <item name="android:background">@color/gray</item>
        <item name="android:layout_marginTop">3dp</item>
    </style>

    <style name="ListDivider">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.6dp</item>
        <item name="android:background">@color/listviewDividerColor</item>
    </style>

    <style name="DividerVertical">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:background">@drawable/abc_list_divider_holo_dark</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:layout_marginBottom">8dp</item>
    </style>

    <style name="ListViewMyAppCompat" parent="android:Widget.ListView">
        <item name="android:paddingLeft">@dimen/card_margin</item>
        <item name="android:paddingRight">@dimen/card_margin</item>
        <item name="android:clipToPadding">false</item>
        <item name="android:scrollbarStyle">outsideOverlay</item>
        <item name="android:divider">@null</item>
        <item name="android:dividerHeight">0dp</item>
        <item name="android:listSelector">?attr/selectableItemBackground</item>
    </style>

    <style name="ListText">
        <item name="android:textAppearance">?android:attr/textAppearanceMedium</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:singleLine">true</item>
    </style>

</resources>